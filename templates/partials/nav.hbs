.space1 a{
    text-decoration: none;
}

.space{
    background: rgba(0, 0, 0, 0.842);
    height: 7vh;
    display: flex;
    align-items: center;
    justify-content: space-between;
    
}
.space2{
    display: flex;
}

#logo{
    padding: 5px;
    height: 3vh;
    background-color: rgb(9, 86, 134);
    border-radius: 2px;
    color: rgba(255, 255, 255, 0.973);
    font-size: 20px;
    font-weight: 700;
    border: 1px solid rgba(255, 255, 255, 0.685);
    margin-left: 10px;
    font-family: 'Libre Franklin', sans-serif;
    text-shadow: -1px 1px 0 #000,1px 1px 0 #000,1px -1px 0 #000,-1px -1px 0 #000;
}

#link1{
    padding: 5px;
    height: 3vh;
    background-color: rgb(9, 86, 134);
    margin-left: 10px;
    border-radius: 2px;
    border: 1px solid rgba(255, 255, 255, 0.685);
}

#link2{
    padding: 5px;
    height: 3vh;
    background-color: rgb(9, 86, 134);
    margin-left: 10px;
    border-radius: 2px;
    margin-right: 10px;
    border: 1px solid rgba(255, 255, 255, 0.685);
}

#link1 a,#link2 a{
    color: white;
    text-decoration: none;
}

#link1:hover,#link2:hover{
    background-color: rgb(11, 124, 194);
}
